#include<iostream>
#include<iomanip>
using namespace std;
const float Pia=3.14;
int main()
{
	int a=21;
	float f=3.456;
	cout<<oct<<a<<dec<<a<<hex<<a<<endl;
	cout<<setiosflags(ios::left)<<setw(6)<<setfill('@')<<a<<endl;
	cout<<f<<endl;
	cout<<setiosflags(ios::showpos)<<setprecision(2)<<f<<endl;
	return 0;
}
